<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>
<div class="well" > 
	<div class ="col-sm-4">
	<div class="conteiner"> 
			<p>
				<strong>Nombre:</strong>
				<%= @student.name %>
			</p>

			<p>
				<strong>Dir school:</strong>
				<p id="star"> <%= @student.dir_school %> </p>
			</p>

			<p>
				<strong>user_id:</strong>
				<%= @student.user_id  %>
			</p>

			<p>
				<strong>Casa dir:</strong>
				<p id="end"> <%= @users.find_by_id(current_user.id).dir %> </p> 
			</p>
			<button  class="btn btn-default">Ver en el mapa</button>
		</div> 

	</div>
	<div class="col-sm-8" ><div id="map"></div></div>

	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC4924wlraUwzko-Ta0YJKCe0hW5wLiP6E&callback=initMap"
	async defer></script>
	<ul class="pager">
		<li> <%= link_to 'Edit', edit_student_path(@student) %>  </li>  
		<li>  <%= link_to 'Back', students_path %>  </li>
	</ul>
</div>

