<p id="notice"><%= notice %></p>

<h1>Listing Student Drivers</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Driver</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_drivers.each do |student_driver| %>
      <tr>
        <td><%= student_driver.student %></td>
        <td><%= student_driver.driver %></td>
        <td><%= link_to 'Show', student_driver %></td>
        <td><%= link_to 'Edit', edit_student_driver_path(student_driver) %></td>
        <td><%= link_to 'Destroy', student_driver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student driver', new_student_driver_path %>
